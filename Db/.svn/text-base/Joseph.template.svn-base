

# Autosave flag
record(bo, "$(P)JOSEPH:AutoSave")
{
    field(PINI, "YES")
    field(ZNAM, "No")
    field(ONAM, "Yes")
	field(FLNK, "$(P)JOSEPH:AutoSaveCopy PP NMS")
}


record(longout, "$(P)JOSEPH:FileNumber")
{
    field(PINI, "YES")
  
    field(VAL, "1")
	field(FLNK, "$(P)JOSEPH:FileNumberCopy PP NMS")

	}


record(waveform, "$(P)JOSEPH:FileName")
{
    field(PINI, "YES")
   
    field(FTVL, "CHAR")
    field(NELM, "256")
}


record(waveform, "$(P)JOSEPH:FilePath")
{
    field(PINI, "YES")
  
    field(FTVL, "CHAR")
    field(NELM, "256")
}






record(dfanout, "$(P)JOSEPH:FilePathCopy")
{
    
    field(DOL, "$(P)JOSEPH:FilePath")
   field(OUTA,"$(P)TIFF1:FilePath PP NMS")
 
    
  	
  
}






record(dfanout, "$(P)JOSEPH:FileNameCopy")
{
    
    field(DOL, "$(P)JOSEPH:FileName")
   field(OUTA,"$(P)TIFF1:FileName PP NMS")
  
    
  	
    field(SCAN, "1 second")
}




record(dfanout, "$(P)JOSEPH:FileNumberCopy")
{
    
    field(INP, "$(P)JOSEPH:FileNumber PP")
   field(OUTA,"$(P)TIFF1:FileNumber PP NMS")
  
    
  	field(OMSL,"closed loop")
      field(SCAN, "1 second")
}



record(dfanout, "$(P)JOSEPH:AutoSaveCopy")
{
    
    field(DOL, "$(P)JOSEPH:AutoSave PP")
   field(OUTA,"$(P)TIFF1:AutoSave PP NMS")
 
    
  	
   
}