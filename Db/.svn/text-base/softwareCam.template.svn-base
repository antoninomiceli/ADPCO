record(longout, "$(P)$(R)sleep_ms")
{
   field(DTYP, "asynInt32")
   field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))sleep_ms")
}

record(longout, "$(P)$(R)is_sleep")
{
   field(DTYP, "asynInt32")
