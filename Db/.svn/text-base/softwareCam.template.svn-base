record(longin, "$(P)$(R)run_counter")
{
   field(PINI, "1")
   field(DTYP, "asynInt32")
   field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))run_counter")
   field(SCAN, "I/O Intr")
}

